{
  "name": "ddm-react-cropperjs",
  "version": "1.0.1",
  "description": "React wrapper around the CropperJS library",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/deseretdigital-ui/ddm-react-cropperjs.git"
  },
  "scripts": {
    "build": "npm run build-js; npm run build-example",
    "build-example": "webpack --config example/webpack.config.js",
    "watch-example": "webpack --config example/webpack.config.js --watch",
    "build-js": "babel ./src -d dist",
    "lint": "eslint src/*",
    "start": "webpack-dev-server --config example/webpack.config.js --content-base example/"
  },
  "keywords": [
    "react",
    "cropper"
  ],
  "license": "MIT",
  "dependencies": {
    "cropperjs": "^0.8.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "^6.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.0",
    "eslint-plugin-react": "^5.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.7.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
