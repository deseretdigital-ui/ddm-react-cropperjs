{
  "name": "ddm-react-cropperjs",
  "version": "2.0.0",
  "description": "React wrapper around the CropperJS library",
  "main": "dist/index.js",
  "repository": "https://github.com/deseretdigital-ui/ddm-react-cropperjs.git",
  "scripts": {
    "build": "yarn run build-js; yarn run build-example",
    "build-example": "webpack --config example/webpack.config.js",
    "watch-example": "webpack --config example/webpack.config.js --watch",
    "build-js": "babel ./src -d dist",
    "lint": "eslint src/*",
    "start": "webpack-dev-server --config example/webpack.config.js --content-base example/"
  },
  "keywords": [
    "react",
    "cropper"
  ],
  "license": "MIT",
  "dependencies": {
    "cropperjs": "^1.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^7.2.0",
    "eslint-plugin-react": "^7.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.5.3",
    "prop-types": "^15.5.10",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "author": "Nate Christensen <natec8@gmail.com>"
}
